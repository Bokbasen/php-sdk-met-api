        "bokbasen/php-sdk-auth": "^1.0"   
     },
     "require-dev": {
        "phpunit/phpunit": "^5",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/mock-client":  "^0.3"
    }
}