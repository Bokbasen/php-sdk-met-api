{
    "name": "bokbasen/php-sdk-met-api",
    "license": "MIT",
    "type": "library",
    "description": "PHP SDK Bokbasen's Metadata API",
    "homepage": "https://github.com/Bokbasen/php-sdk-order-api",
    "autoload": {
        "psr-4" : {
            "Bokbasen\\Metadata\\" : "src"
        }
    },
